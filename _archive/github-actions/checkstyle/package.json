    "@octokit/rest": "^19.0.5",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@actions/io": "^1.1.2",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.26.0",
    "copyfiles": "latest"
  }
